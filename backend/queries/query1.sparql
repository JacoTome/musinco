PREFIX schema: <https://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX musico: <http://purl.org/ontology/musico#> 
PREFIX ex: <http://www.example.audio/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX event: <http://purl.org/NET/c4dm/event.owl#>

SELECT DISTINCT ?musician (AVG(?Duration) as ?avg)
WHERE {
  ?musician musico:multi_instrumentalism_level "multi-instrumentalist" ;
            foaf:based_near ex:Tokyo ;
            musico:in_participation ?participation .

  ?participation a musico:MusicianParticipation ;
                musico:involved_event ?Session.
  ?Session a musico:MusicalSession ;
            schema:isPartOf ?RecreationalActivity .
  ?RecreationalActivity a musico:RecreationalMusicMaking;
                        event:time ?Time.                      
}
GROUP BY ?musician
LIMIT 25